{
	"name": "my-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"postinstall": "npx patch-package",
		"preinstall": "npx patch-package",
		"format": "prettier --plugin-search-dir . --write .",
		"optimize-images": "image-transmutation --run --sourceFolder \"./static\" --targetFolder \"./static\" --inputFormats \"jpg\" --inputFormats \"jpeg\" --inputFormats \"png\" --outputFormats \"webp\" --outputFormats \"avif\" --widths 500 --widths 1000 --enlarge"
	},
	"devDependencies": {
		"@portabletext/svelte": "^2.1.3",
		"@sveltejs/kit": "^1.0.0",
		"@tsconfig/svelte": "^4.0.1",
		"@types/d3-sankey": "^0.12.1",
		"@types/d3-scale": "^4.0.4",
		"@types/lodash-es": "^4.17.9",
		"autoprefixer": "^10.4.13",
		"houdini": "^1.2.27",
		"houdini-svelte": "^1.2.27",
		"image-transmutation": "^0.1.3",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.2",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-vercel": "^3.0.0",
		"classnames": "^2.3.2",
		"d3": "^7.8.4",
		"d3-sankey": "^0.12.3",
		"dateformat": "^5.0.3",
		"flowbite-svelte": "^0.37.4",
		"layercake": "^7.6.1",
		"lodash-es": "^4.17.21",
		"navigation": "^6.1.0",
		"nodemon": "^2.0.22",
		"patch-package": "^8.0.0",
		"radar-chart-d3": "github:alangrafu/radar-chart-d3",
		"svelte-inview": "^4.0.2"
	}
}
